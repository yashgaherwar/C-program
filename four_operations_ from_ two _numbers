#include<stdio.h>

int main()
{
    int a,b,addition,subtraction,division,multiplication;
    printf("enter two values\n");
    scanf("%d%d",&a,&b);
    
    addition=(a+b);
    subtraction=(a-b);
    division=(a/b);
    multiplication=(a*b);
    printf("addition of two numbers :%d\n%d\n%d\n%d\n",addition,subtraction,division,multiplication);
    
    return 0;
    
    
}
